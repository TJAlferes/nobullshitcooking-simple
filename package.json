{
  "name": "nobullshitcooking-ui",
  "version": "1.0.0",
  "description": "UI for No Bullshit Cooking",
  "main": "index.js",
  "proxy": "http://localhost:3003",
  "scripts": {
    "build": "rimraf dist && webpack --config webpack.prod.js --progress --profile --color",
    "start": "webpack-dev-server --config webpack.dev.js",
    "test": "jest --color"
  },
  "jest": {
    "globals": {
      "ts-jest": {
        "babelConfig": true
      }
    },
    "preset": "ts-jest/presets/js-with-babel",
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|scss|sass|less)$": "identity-obj-proxy",
      "^dnd-core$": "dnd-core/dist/cjs",
      "^react-dnd$": "react-dnd/dist/cjs",
      "^react-dnd-html5-backend$": "react-dnd-html5-backend/dist/cjs",
      "^react-dnd-touch-backend$": "react-dnd-touch-backend/dist/cjs",
      "^react-dnd-multi-backend/dist/esm/HTML5toTouch$": "react-dnd-multi-backend/dist/cjs",
      "^react-dnd-test-backend$": "react-dnd-test-backend/dist/cjs",
      "^react-dnd-test-utils$": "react-dnd-test-utils/dist/cjs"
    },
    "setupFilesAfterEnv": [
      "jest-enzyme"
    ],
    "transform": {
      "^.+\\.[t]sx?$": "ts-jest",
      "^.+\\.[j]sx?$": "babel-jest"
    },
    "testEnvironment": "enzyme",
    "testEnvironmentOptions": {
      "enzymeAdapter": "react16"
    }
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.10.0",
    "@babel/core": "^7.10.0",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-export-default-from": "^7.8.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.10.0",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-react-jsx": "^7.9.4",
    "@babel/plugin-transform-runtime": "^7.10.0",
    "@babel/preset-env": "^7.10.0",
    "@babel/preset-react": "^7.10.0",
    "@babel/register": "^7.9.0",
    "@types/enzyme": "^3.10.5",
    "@types/enzyme-adapter-react-16": "^1.0.6",
    "@types/history": "^4.7.6",
    "@types/jest": "^25.2.3",
    "@types/prop-types": "^15.7.3",
    "@types/react": "^16.9.35",
    "@types/react-aria-modal": "^4.0.2",
    "@types/react-dnd-multi-backend": "^5.0.0",
    "@types/react-dom": "^16.9.8",
    "@types/react-image-crop": "^8.1.2",
    "@types/react-redux": "^7.1.9",
    "@types/react-router": "^5.1.7",
    "@types/react-router-dom": "^5.1.5",
    "@types/slate": "^0.47.7",
    "@types/slate-react": "^0.22.9",
    "@types/socket.io-client": "^1.4.33",
    "@types/uuid": "^7.0.4",
    "autoprefixer": "^8.6.5",
    "axios-mock-adapter": "^1.18.1",
    "babel-jest": "^25.5.1",
    "babel-loader": "^8.1.0",
    "brotli-webpack-plugin": "^1.1.0",
    "core-js": "^3.6.5",
