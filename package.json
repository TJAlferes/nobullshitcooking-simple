{
  "name": "nobullshitcooking-ui",
  "version": "1.0.0",
  "description": "UI for No Bullshit Cooking",
  "main": "index.js",
  "proxy": "http://localhost:3003",
  "scripts": {
    "build": "rimraf dist && webpack --config webpack.prod.js --progress --profile --color",
    "generate-imported-component": "imported-components src src/imported.ts",
    "start": "webpack-dev-server --config webpack.dev.js",
    "test": "jest --color"
  },
  "jest": {
    "collectCoverage": true,
    "coverageDirectory": "coverage",
    "globals": {
      "ts-jest": {
        "babelConfig": true
      }
    },
    "globalSetup": "./global-setup.js",
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|scss|sass|less)$": "identity-obj-proxy",
      "^dnd-core$": "dnd-core/dist/cjs",
      "^react-dnd$": "react-dnd/dist/cjs",
      "^react-dnd-html5-backend$": "react-dnd-html5-backend/dist/cjs",
      "^react-dnd-touch-backend$": "react-dnd-touch-backend/dist/cjs",
      "^react-dnd-multi-backend/dist/esm/HTML5toTouch$": "react-dnd-multi-backend/dist/cjs",
      "^react-dnd-test-backend$": "react-dnd-test-backend/dist/cjs",
      "^react-dnd-test-utils$": "react-dnd-test-utils/dist/cjs"
    },
    "preset": "ts-jest/presets/js-with-babel",
    "setupFiles": [
      "<rootDir>/tests/setupTests.ts"
    ],
    "setupFilesAfterEnv": [
      "jest-enzyme"
    ],
    "testEnvironment": "enzyme",
    "testEnvironmentOptions": {
      "enzymeAdapter": "react16"
    },
    "testRegex": "/*.test.tsx?$",
    "transform": {
      "^.+\\.[t]sx?$": "ts-jest",
      "^.+\\.[j]sx?$": "babel-jest"
    }
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.11.6",
    "@babel/core": "^7.11.6",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/plugin-proposal-export-default-from": "^7.10.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.11.0",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-react-jsx": "^7.10.4",
    "@babel/plugin-transform-runtime": "^7.11.5",
    "@babel/preset-env": "^7.11.5",
    "@babel/preset-react": "^7.10.4",
    "@babel/register": "^7.11.5",
    "@types/enzyme": "^3.10.5",
    "@types/enzyme-adapter-react-16": "^1.0.6",
    "@types/history": "^4.7.7",
    "@types/is-url": "^1.2.28",
    "@types/jest": "^26.0.13",
    "@types/prop-types": "^15.7.3",
    "@types/react": "^16.9.49",
    "@types/react-aria-modal": "^4.0.2",
    "@types/react-dnd-multi-backend": "^6.0.0",
    "@types/react-dom": "^16.9.8",
    "@types/react-image-crop": "^8.1.2",
    "@types/react-redux": "^7.1.9",
    "@types/react-router": "^5.1.8",
    "@types/react-router-dom": "^5.1.5",
    "@types/slate": "^0.47.7",
    "@types/slate-react": "^0.22.9",
    "@types/socket.io-client": "^1.4.33",
    "@types/uuid": "^8.3.0",
    "@types/victory": "^33.1.4",
    "autoprefixer": "^9.8.6",
    "axios-mock-adapter": "^1.18.2",
    "babel-jest": "^26.3.0",
    "babel-loader": "^8.1.0",
    "brotli-webpack-plugin": "^1.1.0",
    "core-js": "^3.6.5",
    "css-loader": "^3.6.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.4",
    "enzyme-to-json": "^3.5.0",
    "express": "^4.17.1",
    "extract-css-chunks-webpack-plugin": "^4.7.5",
    "file-loader": "^6.1.0",
    "html-webpack-plugin": "^4.4.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.4.2",
    "jest-cli": "^26.4.2",
    "jest-environment-enzyme": "^7.1.2",
    "jest-enzyme": "^7.1.2",
    "node-sass": "^4.14.1",
    "postcss-loader": "^3.0.0",
    "react-dnd-test-backend": "^11.1.3",
    "react-dnd-test-utils": "^11.1.3",
