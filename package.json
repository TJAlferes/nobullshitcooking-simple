    "jest-cli": "^25.5.4",
    "jest-environment-enzyme": "^7.1.2",
    "jest-enzyme": "^7.1.2",
    "node-sass": "^4.14.1",
    "postcss-loader": "^2.1.6",
    "react-dnd-test-backend": "^7.7.0",
    "react-hot-loader": "^4.12.21",
    "react-test-renderer": "^16.13.1",
    "redux-devtools-extension": "^2.13.8",
    "responsive-loader": "^1.2.0",
    "rimraf": "^2.7.1",
    "sass": "^1.26.5",
    "sass-loader": "^8.0.2",
